<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.UsuariosFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/etPersonAux2"
            android:layout_width="0dp"
            android:layout_height="39dp"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="3dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView12"
            app:layout_constraintEnd_toEndOf="@+id/etPersonFechaNacimiento"
            app:layout_constraintStart_toEndOf="@+id/textView13" />

        <EditText
            android:id="@+id/etPersonObservaciones"
            android:layout_width="0dp"
            android:layout_height="37dp"
            android:layout_marginTop="56dp"
            android:layout_marginEnd="18dp"
            android:layout_marginBottom="56dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/etPersonAux1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            app:layout_constraintTop_toBottomOf="@+id/etPersonLugarEntrega" />

        <EditText
            android:id="@+id/etPersonFechaNacimiento"
            android:layout_width="0dp"
            android:layout_height="38dp"
            android:layout_marginTop="92dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="92dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/etPersonAux2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toTopOf="@+id/etPersonTelefono" />

        <EditText
            android:id="@+id/etPersonAux1"
            android:layout_width="0dp"
            android:layout_height="38dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="46dp"
            android:layout_marginEnd="13dp"
            android:layout_marginBottom="46dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/etPersonAux2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintTop_toTopOf="@+id/etPersonFechaNacimiento" />

        <EditText
            android:id="@+id/etPersonTelefono"
            android:layout_width="0dp"
            android:layout_height="37dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="@+id/etPersonObservaciones"
            app:layout_constraintStart_toStartOf="@+id/etPersonObservaciones" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="7dp"
            android:layout_marginBottom="11dp"
            android:text="PersonName: "
            app:layout_constraintBottom_toTopOf="@+id/txtPersonGivenName"
            app:layout_constraintEnd_toEndOf="@+id/txtPersonName" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:layout_marginBottom="14dp"
            android:text="PersonGivenName: "
            app:layout_constraintBottom_toTopOf="@+id/txtPersonFamilyName"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/txtPersonGivenName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView2"
            app:layout_constraintEnd_toStartOf="@+id/txtPersonFamilyName"
            app:layout_constraintStart_toStartOf="@+id/txtPersonFamilyName" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="7dp"
            android:layout_marginTop="105dp"
            android:text="PersonFamilyName: "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txtPersonFamilyName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView3"
            app:layout_constraintEnd_toStartOf="@+id/txtPersonEmail"
            app:layout_constraintStart_toStartOf="@+id/txtPersonEmail" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="7dp"
            android:text="PersonEmail: "
            app:layout_constraintEnd_toEndOf="@+id/txtPersonEmail"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/txtPersonName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView1"
            app:layout_constraintEnd_toStartOf="@+id/txtPersonGivenName"
            app:layout_constraintStart_toStartOf="@+id/txtPersonGivenName" />

        <TextView
            android:id="@+id/txtPersonEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
            app:layout_constraintEnd_toStartOf="@+id/txtPersonId"
            app:layout_constraintStart_toStartOf="@+id/txtPersonId" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            android:text="PersonId"
            app:layout_constraintEnd_toEndOf="@+id/txtPersonId"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/txtPersonId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
            app:layout_constraintEnd_toStartOf="@+id/etPersonNombreUsuario"
            app:layout_constraintStart_toStartOf="@+id/etPersonNombreUsuario" />

        <EditText
            android:id="@+id/etPersonNombreUsuario"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginEnd="16dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="134dp"
            android:layout_height="24dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="51dp"
            android:layout_marginEnd="2dp"
            android:layout_marginBottom="51dp"
            android:text="PersonNombreUsuario"
            android:textSize="13sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView7"
            app:layout_constraintEnd_toStartOf="@+id/etPersonNombreUsuario"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/txtPersonId" />

        <EditText
            android:id="@+id/etPersonLugarEntrega"
            android:layout_width="0dp"
            android:layout_height="39dp"
            android:layout_marginTop="141dp"
            android:layout_marginBottom="141dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/etPersonFechaNacimiento"
            app:layout_constraintEnd_toEndOf="@+id/etPersonNombreUsuario"
            app:layout_constraintStart_toStartOf="@+id/etPersonNombreUsuario"
            app:layout_constraintTop_toBottomOf="@+id/txtPersonFamilyName" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="PersonLugarEntrega"
            app:layout_constraintBaseline_toBaselineOf="@+id/etPersonLugarEntrega"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="258dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="258dp"
            android:text="PersonTelefono"
            app:layout_constraintBottom_toTopOf="@+id/textView14"
            app:layout_constraintEnd_toStartOf="@+id/etPersonTelefono"
            app:layout_constraintTop_toTopOf="@+id/textView2" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginEnd="1dp"
            android:text="PersonObservaciones"
            app:layout_constraintBaseline_toBaselineOf="@+id/etPersonObservaciones"
            app:layout_constraintEnd_toStartOf="@+id/etPersonObservaciones"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="4dp"
            android:text="PersonFechaNacimiento"
            android:textSize="12sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/etPersonFechaNacimiento"
            app:layout_constraintEnd_toStartOf="@+id/etPersonFechaNacimiento"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="7dp"
            android:text="PersonAux1"
            app:layout_constraintBaseline_toBaselineOf="@+id/etPersonAux1"
            app:layout_constraintEnd_toStartOf="@+id/etPersonFechaNacimiento" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="7dp"
            android:layout_marginBottom="22dp"
            android:text="PersonAux2"
            app:layout_constraintBottom_toTopOf="@+id/txtPersonUltimoPedido"
            app:layout_constraintEnd_toStartOf="@+id/etPersonAux2" />

        <TextView
            android:id="@+id/txtPersonUltimoPedido"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView13"
            app:layout_constraintEnd_toStartOf="@+id/etPersonAux2"
            app:layout_constraintStart_toStartOf="@+id/etPersonAux2" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="24dp"
            android:text="PersonUltimoPedido"
            app:layout_constraintBottom_toTopOf="@+id/txtPersonSaldo"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/txtPersonSaldo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView14"
            app:layout_constraintEnd_toStartOf="@+id/btnActualizar"
            app:layout_constraintStart_toStartOf="@+id/btnActualizar" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="28dp"
            android:text="PersonSaldo"
            app:layout_constraintBottom_toTopOf="@+id/btnActualizar"
            app:layout_constraintEnd_toEndOf="@+id/txtPersonSaldo" />

        <Button
            android:id="@+id/btnActualizar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="34dp"
            android:text="Actualizar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/ivPersonPhoto"
            android:layout_width="78dp"
            android:layout_height="78dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:src="@tools:sample/avatars" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>